
<%- include('./../../includes/header') %>
<!-- =============================================== -->

<%- include('./../../includes/sidebar') %>

<!-- =============================================== -->   
 <div class="content-wrapper">    
   <section class="content">
      <div class="box">
          
          <div class="box-body">


            <%- include('./../../includes/message') %>

        <div class="register-box">
          <div class="register-logo">
            <a href="">Create Users</a>
          </div>

          <div class="register-box-body">

            <form class="form-horizontal" method="post" id="form-books" action="/admin/users/addUsers" enctype="multipart/form-data">
              
              <div class="form-group has-feedback">
                <input type="text" class="form-control" id="add_firstname" name="firstname" placeholder="Firstname">
              </div>
              
              <div class="form-group has-feedback">
                <input type="text" class="form-control" id="add_lastname" name="lastname" placeholder="Lastname">
              </div>

              <div class="form-group has-feedback">
                <input type="text" class="form-control" id="add_username" name="username" placeholder="Username">
              </div>

              <div class="form-group has-feedback">
                <input type="password" class="form-control" id="add_password" name="password" placeholder="Password">
              </div>

              <div class="form-group has-feedback">
                <input type="email"  class="form-control" id="add_email" name="email" placeholder="Email">
              </div>

              <div class="form-group has-feedback">
                <select class="form-control" name="role">
                  <option>Select Role</option>
                  <option value="1">Students</option>
                  <option value="2">Teachers</option>
              </select>
              </div>

              <div class="form-group has-feedback">
                <input type="file" name="image" class="form-control" id="add_email" name="email" placeholder="">
              </div>
                
              <div class="form-group has-feedback">
              
                <button type="submit" name="btn_submit" id="btn-save" class="btn btn-primary pull-right">Save </button>
              </div>

            </form>
          </div>
          <!-- /.form-box -->
        </div>
      </div>
    </section>
</div>


<%- include('./../../includes/footer') %>





